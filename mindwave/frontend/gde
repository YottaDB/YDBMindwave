ADD -SEGMENT TEMP -FILE=$ydb_dir/g/temp.dat
ADD -SEGMENT OUTBOUND -FILE=$ydb_dir/g/outbound.dat
CHANGE -SEGMENT DEFAULT -FILE=$ydb_dir/g/web.dat

ADD -REGION TEMP -NOJOURNAL -RECORD_SIZE=16368 -KEY_SIZE=1019 -DYNAMIC_SEGMENT=TEMP
ADD -REGION OUTBOUND -JOURNAL=(BEFORE_IMAGE,FILE_NAME="$ydb_dir/g/outbound.mjl") -DYNAMIC_SEGMENT=OUTBOUND
CHANGE -REGION DEFAULT -JOURNAL=(BEFORE_IMAGE,FILE_NAME="$ydb_dir/g/web.mjl") -DYNAMIC_SEGMENT=DEFAULT

ADD -NAME CacheTemp* -r=TEMP
ADD -NAME MindWaveOutbound -r=OUTBOUND

SHOW -ALL
